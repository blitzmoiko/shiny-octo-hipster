<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cityproperties.domain">
	<class name="ClientPrivilege" table="C_CLIENT_PRIVILEGE">
        <id name="clientId" type="int">
            <column name="CLIENT_ID" />
            <generator class="foreign">
                <param name="property">Client</param>
            </generator>
        </id>
        <one-to-one name="client" class="Client" constrained="true" />
		<property name="hasView" type="boolean">
			<column name="HAS_VIEW" length="1" not-null="true" />
		</property>
		<property name="hasUpdate" type="boolean">
			<column name="HAS_UPDATE" length="1" not-null="true" />
		</property>
		<property name="hasDelete" type="boolean">
			<column name="HAS_DELETE" length="1" not-null="true" />
		</property>
		<property name="hasInsert" type="boolean">
			<column name="hasInsert" length="1" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
