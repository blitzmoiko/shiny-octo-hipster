<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cityproperties.domain">
	<class name="LetterContent" table="C_LETTER_CONTENT">
		<id name="letterContentId" type="int">
			<column name="L_CONTENT_ID" />
			<generator class="native" />
		</id>
		<property name="content" type="string">
			<column name="CONTENT" length="300" not-null="true" />
		</property>
		<set name="mailTypes" table="C_MAIL_TYPE" inverse="true" lazy="true" fetch="select">
			<key>
				<column name="L_CONTENT_ID" not-null="true" />
			</key>
			<one-to-many class="MailType" />
		</set>
	</class>
</hibernate-mapping>
